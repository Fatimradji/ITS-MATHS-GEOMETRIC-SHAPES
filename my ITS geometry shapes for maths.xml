<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontology/its-geometry#"
     xml:base="http://www.semanticweb.org/ontology/its-geometry"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontology/its-geometry">
        <rdfs:comment>Intelligent Tutoring System for Geometry Shapes - Domain Ontology with Authentication System</rdfs:comment>
        <rdfs:label>ITS Geometry Ontology</rdfs:label>
        <owl:versionInfo>3.0</owl:versionInfo>
    </owl:Ontology>

    <!-- CLASSES -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#User">
        <rdfs:label>User</rdfs:label>
        <rdfs:comment>Base class for all system users</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Student">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:label>Student</rdfs:label>
        <rdfs:comment>A registered student user of the intelligent tutoring system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#GuestUser">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:label>Guest User</rdfs:label>
        <rdfs:comment>A temporary user who continues without registration</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Authentication">
        <rdfs:label>Authentication</rdfs:label>
        <rdfs:comment>User authentication and login system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#LoginSession">
        <rdfs:label>Login Session</rdfs:label>
        <rdfs:comment>Active user session in the system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#UserAccount">
        <rdfs:label>User Account</rdfs:label>
        <rdfs:comment>Student account with credentials</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#GuestSession">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:label>Guest Session</rdfs:label>
        <rdfs:comment>Temporary session for guest users</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#RegisteredSession">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:label>Registered Session</rdfs:label>
        <rdfs:comment>Session for logged-in registered students</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Tutor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:label>Tutor</rdfs:label>
        <rdfs:comment>An intelligent tutor or mentor who assists students</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#AITutor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Tutor"/>
        <rdfs:label>AI Tutor</rdfs:label>
        <rdfs:comment>Artificial Intelligence based tutor</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#HumanTutor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Tutor"/>
        <rdfs:label>Human Tutor</rdfs:label>
        <rdfs:comment>Human teacher or mentor</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#LearningDifficulty">
        <rdfs:label>Learning Difficulty</rdfs:label>
        <rdfs:comment>Areas where students face challenges in learning</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#TutoringSession">
        <rdfs:label>Tutoring Session</rdfs:label>
        <rdfs:comment>An interaction session between student and tutor</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Intervention">
        <rdfs:label>Intervention</rdfs:label>
        <rdfs:comment>Teaching strategies used by tutors to help students</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#GeometricShape">
        <rdfs:label>Geometric Shape</rdfs:label>
        <rdfs:comment>Base class for all geometric shapes</rdfs:comment>
    </owl:Class>

    <!-- ... (Previous geometric shape classes remain the same) ... -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#ThreeDShape">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#GeometricShape"/>
        <rdfs:label>3D Shape</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#TwoDShape">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#GeometricShape"/>
        <rdfs:label>2D Shape</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Cube">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#ThreeDShape"/>
        <rdfs:label>Cube</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Sphere">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#ThreeDShape"/>
        <rdfs:label>Sphere</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Cone">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#ThreeDShape"/>
        <rdfs:label>Cone</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Cylinder">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#ThreeDShape"/>
        <rdfs:label>Cylinder</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Triangle">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#TwoDShape"/>
        <rdfs:label>Triangle</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Rectangle">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#TwoDShape"/>
        <rdfs:label>Rectangle</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Formula">
        <rdfs:label>Formula</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#LearningActivity">
        <rdfs:label>Learning Activity</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://www.semanticweb.org/ontology/its-geometry#Progress">
        <rdfs:label>Progress</rdfs:label>
    </owl:Class>

    <!-- PROPERTIES - AUTHENTICATION SYSTEM -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#hasAccount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Student"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#UserAccount"/>
        <rdfs:label>has account</rdfs:label>
        <rdfs:comment>Student has a registered account</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#hasActiveSession">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:label>has active session</rdfs:label>
        <rdfs:comment>User currently has an active login session</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#usesAuthentication">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Authentication"/>
        <rdfs:label>uses authentication</rdfs:label>
        <rdfs:comment>User utilizes authentication system</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#canLogin">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Student"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Authentication"/>
        <rdfs:label>can login</rdfs:label>
        <rdfs:comment>Student can use login functionality</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#canContinueAsGuest">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Authentication"/>
        <rdfs:label>can continue as guest</rdfs:label>
        <rdfs:comment>User can access system without registration</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#savesProgress">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#RegisteredSession"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Progress"/>
        <rdfs:label>saves progress</rdfs:label>
        <rdfs:comment>Registered sessions save progress permanently</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#hasTemporaryProgress">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#GuestSession"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Progress"/>
        <rdfs:label>has temporary progress</rdfs:label>
        <rdfs:comment>Guest sessions have temporary progress storage</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#convertsToStudent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#GuestUser"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Student"/>
        <rdfs:label>converts to student</rdfs:label>
        <rdfs:comment>Guest user can convert to registered student</rdfs:comment>
    </owl:ObjectProperty>

    <!-- PROPERTIES - TUTOR RELATIONSHIPS -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#hasTutor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Student"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Tutor"/>
        <rdfs:label>has tutor</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#mentorsStudent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Tutor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontology/its-geometry#Student"/>
        <rdfs:label>mentors student</rdfs:label>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontology/its-geometry#hasTutor"/>
    </owl:ObjectProperty>

    <!-- ... (Previous properties remain the same) ... -->

    <!-- DATATYPE PROPERTIES - AUTHENTICATION -->
    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#username">
