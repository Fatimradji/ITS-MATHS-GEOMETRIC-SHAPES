        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>username</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#password">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#UserAccount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>password</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#email">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#UserAccount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>email</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#sessionStartTime">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>session start time</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#sessionEndTime">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>session end time</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#isActive">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#LoginSession"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:label>is active</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#guestId">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#GuestUser"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>guest ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/ontology/its-geometry#registrationDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontology/its-geometry#UserAccount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>registration date</rdfs:label>
    </owl:DatatypeProperty>

